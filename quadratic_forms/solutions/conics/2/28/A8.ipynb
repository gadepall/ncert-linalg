      "source": [
        "# QR Decomposition   \n",
        "import numpy as np\n",
        "A=np.array([[4.,-3.],[6.,-2.]])\n",
        "print(\"Given matrix A is: \\n\",A)\n",
        "Q,R=np.linalg.qr(A)\n",
        "print(\"\\nOrthogonal Matrix Q is :\\n\",Q)\n",
        "#Verification Q^TQ=I\n",
        "print(\"\\nQ^TQ:\\n\",Q.T@Q)\n",
        "\n",
        "print(\"\\nUpper Triangular Matrix R is: \\n\",R)\n",
        "print(\"\\nA=QR=\\n\",Q@R)"
      ],
      "execution_count": 13,
