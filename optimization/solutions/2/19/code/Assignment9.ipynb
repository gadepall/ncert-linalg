   "source": [
    "import numpy as np\n",
    "#orthogonal matrix\n",
    "Q = np.array([[1/np.sqrt(5), 2/np.sqrt(5)],[2/np.sqrt(5), -1/np.sqrt(5)]])\n",
    "#Upper triangular matrix\n",
    "R = np.array([[np.sqrt(5), -2/np.sqrt(5)],[0, 6/np.sqrt(5)]])\n",
    "#Original matrix\n",
    "A = Q@R\n",
    "print(\"Multiplication of Q and R is :\\n\",A)"
   ]
  }
