import matplotlib.pyplot as plt
import numpy as np
import subprocess
import shlex
a=np.empty((2,2))
b=np.empty((2,2))
c=np.empty((2,2))
def matrix1():
	for i in range(2):
		for j in range(2):
			k=i+1
			l=j+1
			a[i][j] = ((k+l)**2)/2
			
	print(a)

def matrix2():
	for i in range(2):
		for j in range(2):
			k=i+1
			l=j+1
			b[i][j] = k/l
			
	print(b)

def matrix3():
	for i in range(2):
		for j in range(2):
			k=i+1
			l=j+1
			c[i][j] = ((k+2*l)**2)/2
			
	print(c)
	
matrix1()
matrix2()
matrix3()
			
	
	
