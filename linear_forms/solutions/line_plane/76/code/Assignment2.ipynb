    "import numpy as np\n",
    "#verification\n",
    "a = np.array([7, -5, 1])\n",
    "b = np.array([1, 2, 3])\n",
    "theta = np.arccos(np.dot(a,b)/(np.linalg.norm(a)*np.linalg.norm(b)))\n",
    "print(\"Angle between the two line in degrees is:\")\n",
    "print(np.degrees(theta))"
   ]
  }
